function checkSpeed(speed) {
    const speedLimit = 70;
    const kmPerDemirtPoint =5;

if (speed <= speedLimit)  {
    console.log("ok");
} else {
    const demeritPoints = math.floor((speed - speedLimit) / kmPerDemirtPoint);
    if (demeritPoints >= 12){
        console.log("license suspended");
} else {
console.log("points:" + demeritPoints);
}

}

}

checkSpeed(80); // Output: Point:2
checkSpeed(65); // Outpit: Ok
checkSpeed(135); // Output: License suspended



